{
  "family": "frontend",
  "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
  "networkMode": "none",
  "containerDefinitions": [
    {
      "name": "ecs-webapp-frontend",
      "image": "uri_of_image_in_ecr",
      "cpu": 1024,
      "memory": 2048,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "",
          "value": ""
        }
      ],
      "secrets": [
        {
          "name": "",
          "valueFrom": ""
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-webapp-frontend",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "frontend"
        }
      }
    }
  ]
}
